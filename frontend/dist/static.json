{
  "headers": {
    "/Images/*": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/Videos/*": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.js": {
      "Cache-Control": "public, max-age=86400, must-revalidate"
    },
    "/*.css": {
      "Cache-Control": "public, max-age=86400, must-revalidate"
    }
  }
}
